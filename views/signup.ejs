<% include ./partials/header %>

<script src="https://use.fontawesome.com/1dec14be15.js"></script>

  <div class="container">
    
  </div>

    <div class="container-fluid stylish-form">

    <div class="stepwizard col-md-12 col-sm-12 col-xs-12">
      <div class="stepwizard-row setup-panel">
        <div class="stepwizard-step">
          <a href="#step-1" type="button" class="step-button btn btn-primary btn-circle">1</a>
          <p>Step 1</p>
        </div>
        <div class="stepwizard-step">
          <a href="#step-2" type="button" class="step-button btn btn-default btn-circle" disabled="disabled">2</a>
          <p>Step 2</p>
        </div>
        <div class="stepwizard-step">
          <a href="#step-3" type="button" class="step-button btn btn-default btn-circle" disabled="disabled">3</a>
          <p>Step 3</p>
        </div>
      </div>
    </div>

      <div class="row mar20" >
        <div class="col-md-12 col-sm-12 col-xs-12">
          <div class="inner-section">
            <form method="POST" action="https://google.co.in">
              <div class="mar20 inside-form">
                <h2 class="font_white text-center signup-text"> SIGN UP</h2>
                <!-- Name -->
                <div class="input-group">
                  <span class="input-group-addon"><i class="fa fa-pencil "></i></span>
                  <input type="text" class="form-control" name="text" placeholder="Name...">
                </div>
                <!--Email --> 
                <div class="input-group">
                  <span class="input-group-addon"><i class="fa fa-envelope "></i></span>
                  <input type="email" class="form-control" name="text" placeholder="Email...">
                </div>
                <!--Phone no -->
                <div class="input-group">
                  <span class="input-group-addon"><i class="fa fa-phone"></i></span>
                  <input type="tel" class="form-control" name="text" placeholder="Phone No...">
                </div>
                 <!-- Date of birth-->
                <div class="input-group date" id="datetimepicker">
                  <span class="input-group-addon"><i class="fa fa-calendar"></i></span>
                  <input type="text" class="form-control" name="text" placeholder="Date of Birth...">
                </div>

                <!-- Gender -->
                <div class="input-group">
                  <div class="maxl">
                    <label style="color:whitesmoke; font-weight:200; margin-left:10px;">Gender</label>
                    <label class="radio inline"> 
                        <input type="radio" name="sex" value="male" checked>
                        <span> Male </span> 
                    </label>
                    <label class="radio inline"> 
                        <input type="radio" name="sex" value="female">
                        <span>Female </span> 
                    </label>
                </div>

                </div>

                <!-- Street -->
                <div class="input-group">
                  <span class="input-group-addon"><i class="fa fa-road" aria-hidden="true"></i></span>
                  <input type="text" class="form-control" name="text" placeholder="Street...">
                </div>
                <!-- Locality -->
                <div class="input-group">
                  <span class="input-group-addon"><i class="fa fa-home"></i></span>
                  <input type="text" class="form-control" name="text" placeholder="Locality...">
                </div>
                <!-- City-->
                <div class="input-group">
                  <span class="input-group-addon"><i class="fa fa-globe"></i></span>
                  <input type="text" class="form-control" name="text" placeholder="City...">
                </div>
                <!-- State -->
                <div class="input-group">
                  <span class="input-group-addon"><i class="fa fa-globe"></i></span>
                  <input type="text" class="form-control" name="text" placeholder="State...">
                </div>
                <!--Pincode -->
                <div class="input-group">
                  <span class="input-group-addon"><i class="fa fa-pencil"></i></span>
                  <input type="text" class="form-control" name="text" placeholder="Pincode...">
                </div>
                <!--Password -->
                <div class="input-group">
                  <span class="input-group-addon"><i class="fa fa-lock "></i></span>
                  <input type="password" class="form-control" name="text" placeholder="Password...">
                </div>
                <!-- Confirm Password -->
                <div class="input-group">
                  <span class="input-group-addon"><i class="fa fa-lock "></i></span>
                  <input type="password" class="form-control" name="text" placeholder="Confirm Password...">
                </div>
                <!-- Gender-->
                <div class="footer text-center">
                  <a href="http://vijender.in/" class="btn btn-neutral btn-round btn-lg">Get Started</a>
                </div>
              </div>
            </form>
          </div>
        </div>
      </div>
    </div>

<% include ./partials/signfoot %>